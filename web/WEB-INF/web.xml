    <servlet>
        <!-- servlet的内部名称 ，可以自定义-->
        <servlet-name>NewsListServlet</servlet-name>
        <!--  servlet类名： 包名+简单类名-->
        <servlet-class>servlet.NewsListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <!-- servlet的内部名称，和上面的名称保持一致-->
        <servlet-name>NewsListServlet</servlet-name>
        <!--  servlet的访问名称： /名称 -->
        <url-pattern>/NewsListServlet</url-pattern>
    </servlet-mapping>
